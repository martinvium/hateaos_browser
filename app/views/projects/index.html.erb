<h1>Projects</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Browse</th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |key,project| %>
      <tr>
        <td><%= project.name %></td>
        <td><%= link_to 'Browse', get_project_resource_path(project) %></td>
      </tr>
    <% end %>
  </tbody>
</table>